<h2> Log In </h2>

<%= simple_form_for(@user) do |f| %>
<div class="form-inputs">
    <%= f.input :name,
                required: true,
                autofocus: true,
                input_html: { autocomplete: "name"} %>
</div>

<div class="form-actions">
  <%= f.button :submit, "Log in" %>
</div>
<% end %>
