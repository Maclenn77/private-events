<%= simple_form_for(:session, url: login_path) do |f| %>
<div class="form-inputs">
    <%= f.input :name,
                required: true,
                autofocus: true,
                input_html: { autocomplete: "name"} %>
</div>

<div class="form-actions">
  <%= f.button :submit, "Log in" %>
</div>
<% end %>
